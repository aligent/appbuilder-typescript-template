    "react-router": "^7.6.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@aligent/ts-code-standards": "^4.0.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@openwhisk/wskdebug": "^1.3.0",
    "@types/node": "^22.19.7",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.2",
    "msw": "^2.7.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "type-fest": "^4.39.1",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest  --passWithNoTests --coverage ./test",
    "e2e": "vitest --collectCoverage=false --testRegex ./e2e",
    "check-types": "npm run check-types:actions && npm run check-types:web && npm run check-types:tests",
    "check-types:actions": "tsc --noEmit --project src/actions/tsconfig.json",
    "check-types:web": "tsc --noEmit --project src/web/tsconfig.json",
    "check-types:tests": "tsc --noEmit --project tests/tsconfig.json"
  },
  "engines": {
    "node": ">=18"
  }
